{
    "name": "matiasdelellis/facerecognition",
    "type": "project",
    "license": "AGPLv3",
    "authors": [
        {
            "name": "Matias De Lellis"
        },
        {
            "name": "Branko Kokanovic"
        }
    ],
    "require": {
        "php": ">=8.3",
        "doctrine/dbal": "*"
    },
    "require-dev": {
        "nextcloud/ocp": "dev-stable31",
        "phpunit/phpunit": "^12",
        "psalm/phar": "^4",
        "scrutinizer/ocular": "^1.8",
        "psr/log": "^3"
    },
    "autoload": {
        "psr-4": {
            "OCA\\FaceRecognition\\": "lib/"
        }
     },

     "autoload-dev": {
         "psr-4": {
                "OCA\\FaceRecognition\\Tests\\Integration\\": "tests/Integration",
                "OCA\\FaceRecognition\\Tests\\Unit\\": "tests/Unit"
          }
     },
    "scripts": {
        "lint": "find . -name \\*.php -not -path './vendor/*' -print0 | xargs -0 -n1 php -l",
        "psalm": "psalm.phar"
    }
}
